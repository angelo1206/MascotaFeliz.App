@page
@model MascotaFeliz.App.Presentacion.Pages.ListModel
@{
}
<h1>Aqui vamos a listar los clientes</h1>

<table class="table">
    
    @foreach (var cliente in Model.Clientes)
    {
        <tr>
            <td>@cliente.nombres</td>
            <td>@cliente.apellidos</td>
            
        </tr>
    }
</table>



